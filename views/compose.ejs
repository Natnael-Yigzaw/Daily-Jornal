<%- include("./partials/header") %>
<div class="container mt-4">
  <h1>Compose</h1>
  <form action="/compose" method="post">
    <div class="form-group">
      <label for="inputtext">Title</label>
      <input type="text" class="form-control" name="posttitle" id="inputtext">
    </div>
    <div class="form-group">
      <label for="inputpost">Post</label>
      <textarea class="form-control" name="postbody" id="posttext" rows="10"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="postbutton">Publish</button>
  </form>
</div>
<%- include("./partials/footer") %>
